port DropDownCollapse from '../../components/DropDownCollapse'\n\nfunction Apropos() {\n  return (\n    <div className={style.aproposcontainer}>\n      <div className={style.aproposbanner}>\n        <img className={style.bannerimage2} src={BackgroundImage2} />\n      </div>\n      <section className={style.dropdownssection}>\n        <div className={style.dropdowncontainer}>\n          <DropDownCollapse\n            titre=\"Fiabilité\"\n            content=\"Les annonces postées sur Kasa garantissent une fiabilité totale. Les photos sont conformes aux logements, et toutes les informations sont régulièrement vérifiées  par nos équipes.\"\n          />\n        </div>\n        <div className={style.dropdowncontainer}>\n          <DropDownCollapse\n            titre=\"Respect\"\n            content=\"La bienveillance fait partie des valeurs fondatrices de Kasa. Tout comportement discriminatoire ou de perturbation du voisinage entraînera une exclusion de notre plateforme.\"\n          />\n        </div>\n        <div className={style.dropdowncontainer}>\n          <DropDownCollapse\n            titre=\"Service\"\n            content=\"Nos équipes se tiennent à votre disposition pour vous fournir une expérience parfaite. N'hésitez pas à nous contacter si vous avez la moindre question.\"\n          />\n        </div>\n        <div className={style.dropdowncontainer}>\n          <DropDownCollapse\n            titre=\"Sécurité\"\n            content=\"La sécurité est la priorité de Kasa. Aussi bien pour nos hôtes que pour les voyageurs, chaque logement correspond aux critères de sécurité établis par nos services. En laissant une note aussi bien à l'hôte qu'au locataire, cela permet à nos équipes de vérifier que les standards sont bien respectés. Nous organisons également des ateliers sur la sécurité domestique pour nos hôtes.\"\n          />\n        </div>\n      </section>\n    </div>\n  )\n}\n\nexport default Apropos\n","/workspaces/Kasa_Martinet_Annesophie/src/pages/404/index.jsx",[],"/workspaces/Kasa_Martinet_Annesophie/src/pages/FicheLogement/index.jsx",["65","66","67","68"],"import React from 'react'\nimport { useParams, Redirect } from 'react-router-dom'\nimport logements from '../../assets/logements.json'\nimport Carrousel from '../../components/Carrousel'\nimport RatingStars from '../../components/RatingStars'\nimport Tags from '../../components/Tags'\nimport * as style from './style.module.css'\n\nfunction FichePropriete() {\n  const id = useParams()\n  const ficheLogement = logements.find((logement) => logement.id === id.id)\n  const {\n    title,\n    location,\n    rating,\n    host,\n    equipments,\n    description,\n    pictures,\n  } = ficheLogement\n\n  return (\n    <>\n      {ficheLogement ? (\n        <section className={style.fiche}>\n          <Carrousel pictures={ficheLogement?.pictures} />\n          <div className={style.fichecontainer}>\n            <div className={style.informations}>\n              <h1 className={style.titrelogement}>{title}</h1>\n              <p className={style.adresse}>{location}</p>\n              <div className={style.tags}>\n                {ficheLogement.tags.map((tag, index) => (\n                  <Tags key={index} getTag={tag} />\n                ))}\n              </div>\n            </div>\n          </div>\n          <div className={style.hoterating}>\n            <RatingStars rating={rating} />\n          </div>\n        </section>\n      ) : (\n        <Redirect replace to=\"/404\" />\n      )}\n    </>\n  )\n}\nexport default FichePropriete\n","/workspaces/Kasa_Martinet_Annesophie/src/components/Header/index.jsx",["69"],"import LogoKasa from '../../assets/LogoKasa.svg'\nimport { Link, NavLink } from 'react-router-dom'\nimport * as style from './style.module.css'\n\nfunction Header() {\n  return (\n    <header>\n      <Link to=\"/\">\n        <img className={style.homelogo} src={LogoKasa} />\n      </Link>\n      <nav className={style.navcontainer}>\n        <NavLink\n          className={style.headerlink}\n          activeClassName={style.activelink}\n          exact\n          to=\"/\"\n        >\n          Accueil\n        </NavLink>\n\n        <NavLink\n          className={style.headerlink}\n          activeClassName={style.activelink}\n          to=\"/about\"\n        >\n          A propos\n        </NavLink>\n      </nav>\n    </header>\n  )\n}\n\nexport default Header\n","/workspaces/Kasa_Martinet_Annesophie/src/components/Footer/index.jsx",["70"],"import LogoKasaBlanc from '../../assets/LogoKasaBlanc.svg'\nimport * as style from './style.module.css'\n\nfunction Footer() {\n  return (\n    <footer className={style.footercontainer}>\n      <img className={style.homelogowhite} src={LogoKasaBlanc} />\n      <span className={style.footertext}>\n        <p>© 2020 Kasa. All rights reserved</p>\n      </span>\n    </footer>\n  )\n}\n\nexport default Footer\n","/workspaces/Kasa_Martinet_Annesophie/src/components/RatingStars/index.jsx",[],"/workspaces/Kasa_Martinet_Annesophie/src/components/Banner/index.jsx",["71"],"import * as style from './style.module.css'\nimport BackgroundImage from '../../assets/BackgroundSectionHeader1.svg'\n\nfunction Banner() {\n  return (\n    <div className={style.bannercontainer}>\n      <img className={style.bannerimage} src={BackgroundImage} />\n      <span className={style.bannertext}>Chez vous, partout et ailleurs</span>\n    </div>\n  )\n}\n\nexport default Banner\n","/workspaces/Kasa_Martinet_Annesophie/src/components/Card/index.jsx",[],"/workspaces/Kasa_Martinet_Annesophie/src/components/DropDownCollapse/index.jsx",[],"/workspaces/Kasa_Martinet_Annesophie/src/components/Carrousel/index.jsx",["72","73","74"],"import React, { useState } from 'react'\nimport * as style from './style.module.css'\nimport arrow from '../../assets/arrowForwardCarrousel.svg'\nimport arrowback from '../../assets/arrowBackwardCarrousel.svg'\n\nconst Carrousel = ({ pictures }) => {\n  const [current, setCurrent] = useState(0)\n  const length = pictures.length\n\n  const nextPic = () => {\n    setCurrent(current === length - 1 ? 0 : current + 1)\n  }\n\n  const prevPic = () => {\n    setCurrent(current === 0 ? length - 1 : current - 1)\n  }\n\n  return (\n    <div className={style.carrousel}>\n      {pictures.map((picture, index) => {\n        return (\n          <div\n            key={index}\n            className={` index === current ? ${style.carrouselpictureactive} : ${style.carrouselpictureinactive}`}\n          >\n            {index === current && (\n              <img\n                src={picture}\n                alt=\"image du logement\"\n                className={style.carrouselpicture}\n              />\n            )}\n          </div>\n        )\n      })}\n\n      {length > 1 ? (\n        <>\n          <div className={style.previous} onClick={prevPic}>\n            <img\n              src={arrowback}\n              alt=\"image précédente\"\n              className={style.arrowback}\n            />\n          </div>\n          <div className={style.next} onClick={nextPic}>\n            <img src={arrow} alt=\"image suivante\" className={style.arrow} />\n          </div>\n        </>\n      ) : null}\n      <div className={style.containerindex}>\n        {current + 1} / {length}\n      </div>\n    </div>\n  )\n}\n\nexport default Carrousel\n","/workspaces/Kasa_Martinet_Annesophie/src/components/Tags/index.jsx",[],"/workspaces/Kasa_Martinet_Annesophie/src/components/Arrow/index.jsx",[],{"ruleId":"75","severity":1,"message":"76","line":9,"column":9,"nodeType":"77","endLine":9,"endColumn":70},{"ruleId":"78","severity":1,"message":"79","line":16,"column":5,"nodeType":"80","messageId":"81","endLine":16,"endColumn":9},{"ruleId":"78","severity":1,"message":"82","line":17,"column":5,"nodeType":"80","messageId":"81","endLine":17,"endColumn":15},{"ruleId":"78","severity":1,"message":"83","line":18,"column":5,"nodeType":"80","messageId":"81","endLine":18,"endColumn":16},{"ruleId":"78","severity":1,"message":"84","line":19,"column":5,"nodeType":"80","messageId":"81","endLine":19,"endColumn":13},{"ruleId":"75","severity":1,"message":"76","line":9,"column":9,"nodeType":"77","endLine":9,"endColumn":58},{"ruleId":"75","severity":1,"message":"76","line":7,"column":7,"nodeType":"77","endLine":7,"endColumn":66},{"ruleId":"75","severity":1,"message":"76","line":7,"column":7,"nodeType":"77","endLine":7,"endColumn":66},{"ruleId":"85","severity":1,"message":"86","line":27,"column":15,"nodeType":"77","endLine":31,"endColumn":17},{"ruleId":"85","severity":1,"message":"86","line":40,"column":13,"nodeType":"77","endLine":44,"endColumn":15},{"ruleId":"85","severity":1,"message":"86","line":47,"column":13,"nodeType":"77","endLine":47,"endColumn":77},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-unused-vars","'host' is assigned a value but never used.","Identifier","unusedVar","'equipments' is assigned a value but never used.","'description' is assigned a value but never used.","'pictures' is assigned a value but never used.","jsx-a11y/img-redundant-alt","Redundant alt attribute. Screen-readers already announce `img` tags as an image. You don’t need to use the words `image`, `photo,` or `picture` (or any specified custom words) in the alt prop."]